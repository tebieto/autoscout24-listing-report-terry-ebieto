{
  "name": "autoscout24-take-home-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "scripts": {
    "test": "yarn jest --silent",
    "start-server": "yarn build && cd build && node .",
    "build-client": "cd ../client && yarn build",
    "build": "tsc --project ./",
    "lint": "eslint . --ext .ts --fix",
    "dev": "yarn test && yarn lint && ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "serve": "yarn build-client && yarn dev",
    "client-install": "cd ../client && yarn install",
    "dependencies": "yarn install && yarn client-install"
  },
  "dependencies": {
    "apollo-server-express": "^2.24.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fast-csv": "^4.3.6",
    "graphql": "^15.5.0",
    "helmet": "^4.6.0",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.5",
    "@types/jest": "^26.0.23",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.6.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-react": "^7.23.2",
    "faker": "^5.5.3",
    "jest": "^27.0.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
}
